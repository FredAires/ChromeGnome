{
  "manifest_version": 3,
  "name": "Chrome Gnome",
  "description": "Manipulates words on page",
  "version": "0.01",
  "permissions": ["activeTab"],
  "icons": {
    // insert path to our extension LOGO
    // "16": "images/icon-16.png",
  },

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
}
