{
  "manifest_version": 3,
  "name": "Chrome Gnome",
  "description": "Manipulates words on page",
  "version": "0.01",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_icon": {
      "16": "images/LoneGnome.png",
      "32": "images/TwoGnomes.png"
    }
  }
}
