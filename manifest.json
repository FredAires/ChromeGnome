{
  "manifest_version": 3,
  "name": "Chrome Gnome",
  "description": "Manipulates words on page",
  "version": "0.01",
  "permissions": ["activeTab"],
  "icons": {
    "OFF": "images/LoneGnome.png"
  },

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_icon": {
      "OFF": "images/LoneGnome.png",
      "ON": "images/TwoGnomes.png"
    }
  }
}
